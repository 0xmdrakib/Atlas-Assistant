{
  "name": "atlas-assistant",
  "version": "1.2.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:migrate": "prisma db push --skip-generate",
    "db:generate": "prisma generate",
    "db:seed": "node prisma/seed.mjs",
    "sources:sync": "tsx scripts/sync-sources.ts",
    "ingest": "tsx scripts/ingest.ts",
    "ingest:loop": "tsx scripts/ingest-loop.ts",
    "setup": "npm run db:migrate && npm run db:seed && npm run sources:sync && npm run ingest",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.19.1",
    "@upstash/qstash": "^2.8.4",
    "lucide-react": "^0.542.0",
    "next": "^14.2.5",
    "next-themes": "^0.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rss-parser": "^3.13.0",
    "fast-xml-parser": "^4.5.0",
    "zod": "^3.23.8",
    "next-auth": "^4.24.11",
    "@next-auth/prisma-adapter": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^20.14.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.5",
    "postcss": "^8.4.39",
    "prisma": "^5.19.1",
    "tailwindcss": "^3.4.7",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4"
  }
}
